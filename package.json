{
  "name": "promise-u",
  "version": "1.2.1",
  "description": "",
  "main": "./obj/index.bundle.js",
  "module": "./obj/index.js",
  "types": "./obj/index.d.ts",
  "scripts": {
    "prepare": "npm run compile",
    "compile": "tsc --module es2015 --moduleResolution node && rollup -c",
    "test": "npm run lint & npm run spec-all",
    "lint": "tslint 'src/**/*.ts'",
    "spec": "tape --require 'ts-node/register'",
    "spec-all": "npm run spec 'src/**/*.spec.ts'",
    "coverage": "nyc --reporter text-summary --reporter lcov --include 'src/**/*.ts' --exclude 'src/**/*.spec.ts' --extension '.ts' npm run spec-all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LuvDaSun/promise-u.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LuvDaSun/promise-u/issues"
  },
  "homepage": "https://github.com/LuvDaSun/promise-u#readme",
  "dependencies": {
    "tslib": "^1.7.1"
  },
  "devDependencies": {
    "@types/blue-tape": "^0.1.31",
    "@types/node": "^6.0.45",
    "@types/tape": "^4.2.30",
    "blue-tape": "^1.0.0",
    "nyc": "^10.3.2",
    "rollup": "^0.55.0",
    "rollup-plugin-commonjs": "^8.2.6",
    "tape": "^4.6.3",
    "ts-node": "^3.0.4",
    "tslint": "^5.2.0",
    "typescript": "^2.3.2"
  }
}
